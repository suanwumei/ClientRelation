<?xml version="1.0"?>  
<!DOCTYPE hibernate-mapping PUBLIC  
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">  
  
<hibernate-mapping package="domain">  

	<class name="Communication" table="communication">  
		<id name="commuId" type="java.lang.Integer" column="commuId">
			<generator class="increment"/>  
		</id>   			
		<property name="commuTime" type="java.lang.String" column="commuTime"/>			
		<property name="commuLocate" type="java.lang.String" column="commuLocate"/>			
		<property name="commuSummary" type="java.lang.String" column="commuSummary"/>			
		<property name="commuDetail" type="java.lang.String" column="commuDetail"/>			
		<property name="ps" type="java.lang.String" column="ps"/>			
			
	</class>  
 
</hibernate-mapping>  

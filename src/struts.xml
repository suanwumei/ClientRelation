<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<constant name="struts.custom.i18n.resources" value="mess"></constant>
	<constant name="struts.i18n.encoding" value="UTF-8"></constant>
	
	<package name="struts" extends="struts-default" namespace="">
		<action name="login" class="action.ClientManagerAction" method="clientManagerLogin">
			<result name="success">/center.jsp</result>
			<result name="failed">/index.jsp</result>
		</action>
		<action name="salseChanceCreate" class="action.SalesChanceAction" method="salseChanceCreate">
			<result name="success">/login.jsp</result>
			<result name="failed">/index.jsp</result>
		</action>
		<action name="salesChanceToCreate" class="action.SalesChanceAction" method="salesChanceToCreate">
			<result name="success">/saleschance_new.jsp</result>
			<result name="failed">/index.jsp</result>
		</action>
		<action name="salesChanceToCenter" class="action.SalesChanceAction" method="salesChanceToCenter">
			<result name="success">/saleschance_center.jsp</result>
			<result name="failed">/index.jsp</result>
		</action>
		<action name="salesChanceToModify" class="action.SalesChanceAction" method="salesChanceToModify">
			<result name="success">/saleschance_modify.jsp</result>
			<result name="failed">/index.jsp</result>
		</action>
		<action name="salesChanceModify" class="action.SalesChanceAction" method="salesChanceModify">
			<result name="success">/index.jsp</result>
			<result name="failed">/index.jsp</result>
		</action>
	</package>
</struts>    

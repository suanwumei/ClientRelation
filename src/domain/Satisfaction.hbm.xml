<?xml version="1.0"?>  
<!DOCTYPE hibernate-mapping PUBLIC  
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">  
  
<hibernate-mapping package="domain">  

	<class name="Satisfaction" table="satisfaction">  
		<id name="satisfactionId" type="java.lang.Integer" column="satisfactionId">
			<generator class="increment"/>  
		</id>  
		<property name="satisfactionName" type="java.lang.String" column="satisfactionName"/>			
			
	</class>  
 
</hibernate-mapping>  
